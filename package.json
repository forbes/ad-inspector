    "Anne Zhou <azhou@forbes.com>",
    "Aaron Romel <aromel@forbes.com>"
  ],
  "license": "MIT",
  "devDependencies": {
    "autoprefixer": "9.1.5",
    "babel-core": "6.26.3",
    "babel-loader": "7.1.5",
    "babel-preset-env": "1.7.0",
    "css-loader": "1.0.0",
    "eslint": "5.6.1",
    "http-server": "0.11.1",
    "postcss-loader": "3.0.0",
    "style-loader": "0.23.1",
    "webpack": "4.20.2",
    "webpack-cli": "3.1.2"
